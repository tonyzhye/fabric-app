{
  "name": "fabric-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start-with-map",
    "start_": "run-p lint:watch start-with-map",
    "build": "run-s clean lint build-with-map remove-map",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build",
    "start-with-map": "react-scripts start",
    "build-with-map": "react-scripts build",
    "remove-map": "rimraf build/**/*.map",
    "clean": "rimraf build",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "lint:watch": "chokidar src/**/*.ts src/**/*.tsx  -c \"npm run lint\" --initial --verbose",
    "analyze-map": "source-map-explorer build/static/js/13.*",
    "analyze": "run-s build analyze-map"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "classnames": "^2.2.6",
    "es6-promise": "^4.2.5",
    "js-cookie": "^2.2.0",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-router": "^4.0.5",
    "mobx-utils": "^5.1.0",
    "office-ui-fabric-react": "^6.118.0",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-beautiful-dnd": "^10.0.3",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.6",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.10",
    "@types/js-cookie": "^2.2.0",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.25.46",
    "@types/react": "^16.7.18",
    "@types/react-beautiful-dnd": "^10.0.0",
    "@types/react-dom": "^16.0.11",
    "@types/react-loadable": "^5.4.3",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "chokidar-cli": "^1.2.1",
    "cross-env": "^5.2.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "serve": "^10.1.1",
    "source-map-explorer": "^1.6.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
